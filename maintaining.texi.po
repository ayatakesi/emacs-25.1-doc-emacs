msgid "Searching and replacing identifiers."
msgstr ""

#. type: node
#: original_texis/maintaining.texi:1770 original_texis/maintaining.texi:1987
#, no-wrap
msgid "List Identifiers"
msgstr ""

#. type: menuentry
#: original_texis/maintaining.texi:1770
msgid "Listing identifiers and completing on them."
msgstr ""

