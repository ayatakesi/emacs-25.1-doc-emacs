#: original_texis/maintaining.texi:1770 original_texis/maintaining.texi:1851
#, no-wrap
msgid "Xref Commands"
msgstr ""

#. type: menuentry
#: original_texis/maintaining.texi:1770
msgid "Commands in the @file{*xref*} buffer."
msgstr ""

